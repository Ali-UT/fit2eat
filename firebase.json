{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "fit2eat",
          "appId": "1:79543153509:android:87ba0c3145ddf89a47a78c",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "fit2eat",
          "appId": "1:79543153509:ios:8e8dd1ae741a253d47a78c",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "fit2eat",
          "configurations": {
            "android": "1:79543153509:android:87ba0c3145ddf89a47a78c",
            "ios": "1:79543153509:ios:8e8dd1ae741a253d47a78c",
            "web": "1:79543153509:web:1e6488367b29022047a78c"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ]
}
